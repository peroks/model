{
	"name": "peroks/model",
	"type": "library",
	"description": "Models: Classes with built-in data validation",
	"minimum-stability": "stable",
	"version": "1.0.0",
	"license": "MIT",
	"keywords": [
		"json",
		"data-validation",
		"rest-api",
		"arrayobject"
	],
	"authors": [
		{
			"name": "Per Egil Roksvaag",
			"homepage": "https://github.com/peroks"
		}
	],
	"autoload": {
		"psr-4": {
			"Peroks\\Model\\": "src/"
		}
	},
	"require": {
		"php": "^8.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^10"
	},
	"suggest": {
		"peroks/model-store": "Permanent data store for models"
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true
	},
	"scripts": {
		"model-test": "./vendor/bin/phpunit tests"
	}
}
